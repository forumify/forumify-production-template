{
    "type": "project",
    "license": "OSL-3.0",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "forumify/forumify-platform": "^0.1",
        "symfony/flex": "^2",
        "symfony/runtime": "6.4.*"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.4.*",
            "endpoint": [
                "https://api.github.com/repos/forumify/flex-recipes/contents/index.json?ref=flex/main",
                "flex://defaults"
            ],
            "docker": false
        }
    }
}
